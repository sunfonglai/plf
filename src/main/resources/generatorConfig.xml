<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >

<generatorConfiguration>


    <context id="context1" targetRuntime="MyBatis3">
        <!-- 运行环境配置 -->
        <plugin type="org.mybatis.generator.plugins.SerializablePlugin"></plugin>
        <!-- 生成注释配置 -->
        <commentGenerator type="">
            <!-- 是否取消注释 -->
            <property name="suppressAllComments" value="false" />
            <!-- 数据库注释支持 -->
            <property name="addRemarkComments" value="true"/>
            <!-- 时间格式设置 -->
            <property name="dateFormat" value="yyyy-MM-dd HH:mm:ss"/>
        </commentGenerator>
        <!-- 连接数据库 -->
        <jdbcConnection driverClass="oracle.jdbc.driver.OracleDriver" connectionURL="jdbc:oracle:thin:@192.168.29.134:1521:orcl" userId="plf" password="1" />
        <!-- 数值类型数据是否全部使用BigDecimal类型 -->
        <javaTypeResolver type="com.sunfong.plf.util.mybaits.MapperTypeResolver">
            <property name="forceBigDecimals" value="false" />
        </javaTypeResolver>
        <!-- 生成entity类 -->
        <javaModelGenerator targetPackage="com.sunfong.plf.dob.entity.privilege" targetProject="src/main/java">
            <property name="enableSubPackages" value="false" />
            <property name="trimStrings" value="true" />
        </javaModelGenerator>
        <!-- 生成xml映射文件 -->
        <sqlMapGenerator targetPackage="static.plf.mapper.privilege" targetProject="src/main/resources">
            <property name="enableSubPackages" value="false" />
            <property name="isMergeable" value="false" />
        </sqlMapGenerator>
        <!-- 生成dao类 -->
        <javaClientGenerator targetPackage="com.sunfong.plf.mapper.privilege" targetProject="src/main/java" type="XMLMAPPER">
            <property name="enableSubPackages" value="false" />
        </javaClientGenerator>
        <!-- 需要生产po类的表 -->
        <table  tableName="PLF_MENU" domainObjectName="PlfMenu" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false">
            <!-- 主键生成方式 -->
            <!--<generatedKey column="source_id" sqlStatement="select seq_t_user.nextval from dual" identity="true" />-->
            <!-- 列名去除前缀 -->
            <!--<columnRenamingRule searchString="^[^_]+" replaceString="" />-->
        </table>

    </context>
</generatorConfiguration>